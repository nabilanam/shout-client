{
  "extends": ["eslint:recommended", "react-app"],
  "rules": {
    "semi": ["error", "never"],
    "indent": ["error", 2],
    "curly": ["error", "all"],
    "linebreak-style": ["error", "unix"],
    "no-sequences": "off",
    "jsx-a11y/anchor-is-valid": "off"
  },
  "globals": {
    "CONFIG": false
  }
}
